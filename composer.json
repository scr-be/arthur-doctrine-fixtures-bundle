{
    "name"        : "scr-be/arthur-doctrine-fixtures-bundle",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/arthur-doctrine-fixtures-bundle",
    "description" : "A set of classes, centered around `AbstractFixture` and its children, that allows for a simple implementation of Doctrine ORM fixture loading.",
    "support" : {
        "email"  : "rmf@scr.be",
        "source" : "https://scr.be/arthur-doctrine-fixtures-bundle/git",
        "issues" : "https://scr.be/arthur-doctrine-fixtures-bundle/issues",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "arthur-doctrine-fixtures-bundle", "scribe-arthur-doctrine-fixtures-bundle", "doctrine", "library", "scr.be", "scribe", "scribenet", "scribe-software", "scribe-systems"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                               : ">=5.6.4,~5.6|~7.0",
        "scr-be/wonka-bundle"               : "~0.3",
        "scr-be/arthur-doctrine-library"    : "~0.3",
        "scr-be/augustus-primitive-library" : "~0.2",
        "symfony/dependency-injection"      : "~2.8|~3.0",
        "doctrine/common"                   : "~2.5",
        "doctrine/data-fixtures"            : "~1.1",
        "doctrine/doctrine-bundle"          : "~1.6",
        "doctrine/doctrine-fixtures-bundle" : "~2.3",
        "hoa/math"                          : "~0.1"
    },
    "require-dev" : {
        "symfony/config"         : "~2.8|~3.0",
        "symfony/yaml"           : "~2.8|~3.0",
        "symfony/finder"         : "~2.8|~3.0",
        "symfony/monolog-bundle" : "~2.8|~3.0",
        "symfony/monolog-bridge" : "~2.8|~3.0",
        "symfony/debug-bundle"   : "~2.8|~3.0",
        "doctrine/orm"           : "~2.5",
        "phpunit/phpunit"        : "~5.0",
        "codacy/coverage"        : "~1.0",
        "satooshi/php-coveralls" : "~1.0"
    },
    "conflict" : {
        "scribe/mantle-bundle"                    : "<2.3.0",
        "scr-be/arthur-doctrine-fixtures-library" : "*"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Arthur\\DoctrineFixturesBundle\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Arthur\\DoctrineFixturesBundle\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "process-timeout"        : 120,
        "bin-dir"                : "bin",
        "cache-files-ttl"        : 60,
        "classmap-authoritative" : true,
        "github-expose-hostname" : false,
        "notify-on-install"      : false,
        "discard-changes"        : "stash"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.4-dev"
        }
    }
}
