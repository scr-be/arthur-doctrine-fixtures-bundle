{
    "name"        : "scr-be/arthur-doctrine-fixtures-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/arthur-doctrine-fixtures-library",
    "description" : "A set of classes, centered around `AbstractFixture` and its children, that allows for a simple implementation of Doctrine ORM fixture loading.",
    "support" : {
        "email"  : "rmf@scr.be",
        "source" : "https://scr.be/arthur-doctrine-fixtures-library/git",
        "issues" : "https://scr.be/arthur-doctrine-fixtures-library/issues",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "arthur-doctrine-fixtures-library", "scribe-arthur-doctrine-fixtures-library", "doctrine", "library", "scr.be", "scribe", "scribenet", "scribe-software", "scribe-systems"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                               : ">=5.6.4,~5.6|~7.0",
        "scr-be/wonka-library"              : "~0.1",
        "scr-be/arthur-doctrine-library"    : "~0.1",
        "scr-be/augustus-primitive-library" : "~0.1",
        "symfony/dependency-injection"      : "~2.8|~3.0",
        "symfony/config"                    : "~2.8|~3.0",
        "symfony/yaml"                      : "~2.8|~3.0",
        "symfony/finder"                    : "~2.8|~3.0",
        "doctrine/common"                   : "~2.5",
        "doctrine/orm"                      : "~2.5",
        "doctrine/data-fixtures"            : "~1.1",
        "doctrine/doctrine-bundle"          : "~1.6",
        "hoa/math"                          : "~0.1"
    },
    "require-dev" : {
        "phpunit/phpunit"        : "~5.0",
        "codacy/coverage"        : "~1.0",
        "satooshi/php-coveralls" : "~0.7",
        "symfony/config"         : "~2.8"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.2.0"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Doctrine\\DataFixtures\\" : "lib/DataFixtures/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Doctrine\\Tests\\DataFixtures\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.1-dev"
        }
    }
}
